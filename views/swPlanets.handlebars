<!--  PAGE BODY -->
        <main class="mainContent
                    col-desktop-7
                    col-tablet-8
                    col-mobile-12">

            <section class="json">
                <h1>gostaria de saber um pouco mais sobre STAR WARS?</h1>

                <!-- API STAR WARS -->
                <div class="buttonsApi">

                    <button><a href="/swPeople">pessoas</a></button>
                    <button><a href="/swPlanets">planetas</a></button>
                    <button><a href="/swStarships">naves</a></button>
                    <button><a href="/swFilms">filmes</a></button>
                    <button><a href="/swSpecies">espécies</a></button>
                    <button><a href="/swVehicles">veículos</a></button>

                </div>

                <!-- CARD STAR WARS -->
                <div class="api">
                    <figure>
                        <img src="../img/sistemaSolar/starWars.jpg" id="img">

                        <ul>
                        {{#each list}}
                            <h2>{{name}}</h2>

                                <li><b>PERÍODO DE ROTAÇÃO:</b> <i>{{rotation_period}}</i></li>
                                <li><b>ÓRBITA:</b> <i>{{orbital_period}}</i></li>
                                <li><b>DÍÂMETRO:</b> <i>{{diameter}}</i></li>
                                <li><b>CLIMA:</b> <i>{{climate}}</i></li>
                                <li><b>GRAVIDADE:</b> <i>{{gravity}}</i></li>
                                <li><b>TERRENO:</b> <i>{{terrain}}</i></li>
                                <li><b>ÁGUA DA SUPERFÍCIE:</b> <i>{{surface_water}}</i></li>
                                <li><b>POPULAÇÃO:</b> <i>{{population}}</i></li>

                        {{/each}}
                        <ul>

                    </figure>
                </div>

                <!-- ADMIN SECTION -->
                <div id="link">
                    <a href="/login">área admin</a>
                </div>
            </section>
        </main>